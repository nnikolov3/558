<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:id="@+id/main"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  android:background="#00E5FF"
  android:foregroundTint="#000000"
  android:outlineAmbientShadowColor="#C6FF00"
  android:outlineSpotShadowColor="#3D5AFE"
  android:textAlignment="center"
  android:visibility="visible"
  tools:context=".ui.main.MainFragment"
  tools:visibility="visible">
    <androidx.constraintlayout.widget.ConstraintLayout
      android:id="@+id/scrool_view"
      android:layout_width="0dp"
      android:layout_height="0dp"
      android:layout_marginTop="16dp"
      android:layout_marginBottom="16dp"
      android:layout_marginStart="30dp"
      android:layout_marginEnd="30dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent">
        <Button
          android:id="@+id/led2"
          android:layout_width="150dp"
          android:layout_height="wrap_content"
          android:layout_marginStart="2dp"
          android:background="#FFFFFF"
          android:elegantTextHeight="true"
          android:outlineAmbientShadowColor="#C6FF00"
          android:shadowColor="#000000"
          android:text="@string/led1"
          android:textAlignment="center"
          android:textAllCaps="true"
          android:textSize="18sp"
          android:visibility="visible"
          app:layout_constraintBaseline_toBaselineOf="@+id/led1"
          app:layout_constraintEnd_toStartOf="@+id/led1"
          app:layout_constraintStart_toStartOf="parent" />
        <Button
          android:id="@+id/led1"
          android:layout_width="150dp"
          android:layout_height="wrap_content"
          android:layout_marginBottom="17dp"
          android:layout_marginEnd="9dp"
          android:background="#FFFFFF"
          android:elegantTextHeight="true"
          android:outlineAmbientShadowColor="#C6FF00"
          android:shadowColor="#000000"
          android:text="@string/led2"
          android:textAlignment="center"
          android:textAllCaps="true"
          android:textSize="18sp"
          android:visibility="visible"
          app:layout_constraintBottom_toTopOf="@+id/led1_status"
          app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintStart_toEndOf="@+id/led2"
          app:layout_constraintTop_toBottomOf="@+id/humid" />
        <TextView
          android:id="@+id/temp_c"
          android:layout_width="319dp"
          android:layout_height="wrap_content"
          android:layout_marginTop="29dp"
          android:layout_marginBottom="31dp"
          android:layout_marginEnd="16dp"
          android:background="#FFC400"
          android:elegantTextHeight="true"
          android:outlineAmbientShadowColor="#C6FF00"
          android:shadowColor="#000000"
          android:text="@string/temp_c"
          android:textAlignment="center"
          android:textAllCaps="true"
          android:textColor="#000000"
          android:textSize="24sp"
          android:visibility="visible"
          app:layout_constraintBottom_toTopOf="@+id/temp_f"
          app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintTop_toTopOf="parent" />
        <TextView
          android:id="@+id/humid"
          android:layout_width="319dp"
          android:layout_height="wrap_content"
          android:layout_marginBottom="18dp"
          android:layout_marginEnd="16dp"
          android:background="#FFC400"
          android:elegantTextHeight="true"
          android:outlineAmbientShadowColor="#C6FF00"
          android:shadowColor="#000000"
          android:text="@string/humid"
          android:textAlignment="center"
          android:textAllCaps="true"
          android:textColor="#000000"
          android:textSize="24sp"
          android:visibility="visible"
          app:layout_constraintBottom_toTopOf="@+id/led1"
          app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintTop_toBottomOf="@+id/temp_f" />
        <TextView
          android:id="@+id/temp_f"
          android:layout_width="319dp"
          android:layout_height="wrap_content"
          android:layout_marginBottom="25dp"
          android:layout_marginEnd="20dp"
          android:background="#FFC400"
          android:elegantTextHeight="true"
          android:outlineAmbientShadowColor="#C6FF00"
          android:shadowColor="#000000"
          android:text="@string/temp_f"
          android:textAlignment="center"
          android:textAllCaps="true"
          android:textColor="#000000"
          android:textSize="24sp"
          android:visibility="visible"
          app:layout_constraintBottom_toTopOf="@+id/humid"
          app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintTop_toBottomOf="@+id/temp_c" />
        <TextView
          android:id="@+id/led1_status"
          android:layout_width="150dp"
          android:layout_height="wrap_content"
          android:layout_marginEnd="9dp"
          android:background="#76FF03"
          android:elegantTextHeight="true"
          android:outlineAmbientShadowColor="#C6FF00"
          android:shadowColor="#000000"
          android:text="@string/led1_status"
          android:textAlignment="center"
          android:textAllCaps="true"
          android:textColor="#000000"
          android:textSize="24sp"
          android:visibility="visible"
          app:layout_constraintBottom_toBottomOf="parent"
          app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintStart_toEndOf="@+id/led2_status"
          app:layout_constraintTop_toBottomOf="@+id/led1" />
        <TextView
          android:id="@+id/led2_status"
          android:layout_width="150dp"
          android:layout_height="wrap_content"
          android:layout_marginEnd="3dp"
          android:background="#76FF03"
          android:elegantTextHeight="true"
          android:outlineAmbientShadowColor="#C6FF00"
          android:shadowColor="#000000"
          android:text="@string/led2_status"
          android:textAlignment="center"
          android:textAllCaps="true"
          android:textColor="#000000"
          android:textSize="24sp"
          android:visibility="visible"
          app:layout_constraintBaseline_toBaselineOf="@+id/led1_status"
          app:layout_constraintEnd_toStartOf="@+id/led1_status"
          app:layout_constraintStart_toStartOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>